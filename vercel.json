{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/sveltekit"
    },
    {
      "src": "api/**/*.py",
      "use": "@vercel/python",
      "config": {
        "memory": 1024,
        "excludeFiles": "{**/tests, public, frontend}/**"
      }
    }
  ]
}