{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/sveltekit"
    },
    {
      "src": "api/**/*.py",
      "use": "@vercel/python"
    }
  ],
  "functions": {
    "api/**": {
      "excludeFiles": "{**/tests, public, frontend}/**",
      "memory": 1024
    }
  }
}
